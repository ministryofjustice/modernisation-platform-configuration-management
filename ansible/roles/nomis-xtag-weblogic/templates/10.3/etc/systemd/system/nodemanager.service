[Unit]
Description=Node Manager controls the WebLogic Server runtime lifecycle
After=network.target
           
[Service]
User=weblogic
Group=javainstall
Type=simple
ExecStart=/home/oracle/admin/scripts/control_NodeManager.sh start >/dev/null 2>/dev/null &
ExecStop=/home/oracle/admin/scripts/control_NodeManager.sh stop >/dev/null 2>/dev/null &
PIDFile=/home/oracle/admin/scripts/nodemanager.process.id
Restart=on-failure
RestartSec=1

[Install]
WantedBy=default.target
